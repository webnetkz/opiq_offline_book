<script>
    //set width of videoBanner to control it's height - want it to be visible without scrolling
    window.addEventListener('load', setVideoArea, false);
    // window.addEventListener('resize', setVideoArea, false);
    function setVideoArea() {
        var viewportHeight = window.innerHeight;
        var viewportWidth = window.innerWidth;
        var videoBanner = document.querySelector('.video-banner');
        //set default to H
        videoBanner.src = "/salespages/blender-bottle/img/bb-video-H-no-audio.mp4";
        // override
        var deviceType = /iPad/.test(navigator.userAgent) ? "t" : /Mobile|iP(hone|od)|Android|BlackBerry|IEMobile|Silk/.test(navigator.userAgent) ? "m" : "d";
        // if(viewportWidth < viewportHeight && (viewportHeight < 1000 && viewportWidth < 600)){
        if (deviceType === "m") {
            videoBanner.src = "/salespages/blender-bottle/img/bb-video-V-no-audio.mp4"
        } else {
            videoBanner.src = "/salespages/blender-bottle/img/bb-video-H-no-audio.mp4";
        }
    }

    // window.addEventListener('load', setTargetArea, false);
    window.addEventListener('scroll', setTargetArea, false);
    window.addEventListener('resize', setTargetArea, false);

    function setTargetArea() {
        var viewportHeight = window.innerHeight;
        var viewportCenter = viewportHeight/2;
        var targetHeight = 200;
        var targetAreaTop = viewportCenter - (targetHeight/2);
        var targetAreaBtm = targetAreaTop + targetHeight;
        var yOffset = window.pageYOffset;
        var targetEl = document.querySelectorAll('.content-section');

        for(let i=0; i < targetEl.length; i++) {
            let activeEl = targetEl[i];
            if (isInTargetArea(activeEl,targetAreaTop, targetAreaBtm, yOffset)) {
                if (!activeEl.classList.contains('animate')){
                    activeEl.classList.add('animate');
                    let animList = activeEl.querySelectorAll('.anim-list li');
                    for(let i=0; i < animList.length; i++) {
                        setTimeout(function() {
                            let curEl = animList[i];
                            curEl.classList.add('show');
                        }, 120 * parseInt(i));
                    }
                    let animRedGlow = activeEl.querySelectorAll('.anim-red-glow');
                    for(let i=0; i < animRedGlow.length; i++) {
                        let curEl = animRedGlow[i];
                        // add class 'show' only if nonexistent, remove after 10s
                        if(!curEl.classList.contains('show')){
                            curEl.classList.add('show');
                            setTimeout(function() {
                                curEl.classList.remove('show');
                            }, 10000);
                        }
                    }
                    let animRedBlack = activeEl.querySelectorAll('.anim-red-black');
                    for(let i=0; i < animRedBlack.length; i++) {
                        let curEl = animRedBlack[i];
                       // add class 'show' only if nonexistent, remove after 10s
                       if(!curEl.classList.contains('show')){
                            curEl.classList.add('show');
                            setTimeout(function() {
                                curEl.classList.remove('show');
                            }, 5000);
                        }
                    }
                }

            } else {
                if (activeEl.classList.contains('animate')){
                    setTimeout(function(){
                        activeEl.classList.remove('animate');
                    }, 2000);
                }
            }
        }

    }

    function isInTargetArea(activeEl, targetAreaTop, targetAreaBtm, yOffset) {
        var elTop = activeEl.getBoundingClientRect().top;
        var elBtm = elTop + activeEl.offsetHeight;
        return elBtm >= targetAreaTop && elTop <= targetAreaBtm;
    }

    var bottlePrice1 = "14.99";
    var bottlePrice2 = "29.98";
    var bottlePrice3 = "44.97";
    var bottlePrice4 = "59.96";
    var bottlePrice5 = "74.95";
