    // Quantity Component
    function increaseValue() {
        var value = parseInt(document.getElementById('quantity-btm').value, 10);
        value = isNaN(value) ? 1 : value;
        value++;
        value > 10 ? value = 10 : '';
        let quantityValue = document.querySelectorAll('.quantity-value');
        for(let i=0; i < quantityValue.length; i++) {
            quantityValue[i].value = value;
            // let buyBtn1 = document.querySelector('.buy-btn-1');
            // let buyBtn2 = document.querySelector('.buy-btn-2');
            let buyBtn3 = document.querySelector('.buy-btn-3');
            let curPrice = eval('bottlePrice' + value);
            if(value > 1) {
                // buyBtn1.dataset.id = 'blender_bottle_' + value + 'qty';
                // buyBtn2.dataset.id = 'blender_bottle_' + value + 'qty';
                buyBtn3.dataset.id = 'blender_bottle_' + value + 'qty';
                // buyBtn1.href = '/order-form/blender-bottle-' + value + 'qty';
                // buyBtn2.href = '/order-form/blender-bottle-' + value + 'qty';
                buyBtn3.href = '/order-form/blender-bottle-' + value + 'qty';
                // buyBtn1.dataset.price = curPrice;
                // buyBtn2.dataset.price = curPrice;
                buyBtn3.dataset.price = curPrice;
            } else {
                // buyBtn1.dataset.id = 'sku_blender_bottle';
                // buyBtn2.dataset.id = 'sku_blender_bottle';
                buyBtn3.dataset.id = 'sku_blender_bottle';
                // buyBtn1.href = '/order-form/blender-bottle';
                // buyBtn2.href = '/order-form/blender-bottle';
                buyBtn3.href = '/order-form/blender-bottle';
                // buyBtn1.dataset.price = curPrice;
                // buyBtn2.dataset.price = curPrice;
                buyBtn3.dataset.price = curPrice;
            }
        }
    }

    function decreaseValue() {
        var value = parseInt(document.getElementById('quantity-btm').value, 10);
        value = isNaN(value) ? 2 : value;
        value < 2 ? value = 2 : '';
        value--;
        let quantityValue = document.querySelectorAll('.quantity-value');
        
        for(let i=0; i < quantityValue.length; i++) {
            quantityValue[i].value = value;
            // let buyBtn1 = document.querySelector('.buy-btn-1');
            // let buyBtn2 = document.querySelector('.buy-btn-2');
            let buyBtn3 = document.querySelector('.buy-btn-3');
            let curPrice = eval('bottlePrice' + value);
            if(value > 1) {
                // buyBtn1.dataset.id = 'blender_bottle_' + value + 'qty';
                // buyBtn2.dataset.id = 'blender_bottle_' + value + 'qty';
                buyBtn3.dataset.id = 'blender_bottle_' + value + 'qty';
                // buyBtn1.href = '/order-form/blender-bottle-' + value + 'qty';
                // buyBtn2.href = '/order-form/blender-bottle-' + value + 'qty';
                buyBtn3.href = '/order-form/blender-bottle-' + value + 'qty';
                // buyBtn1.dataset.price = curPrice;
                // buyBtn2.dataset.price = curPrice;
                buyBtn3.dataset.price = curPrice;
            } else {
                // buyBtn1.dataset.id = 'sku_blender_bottle';
                // buyBtn2.dataset.id = 'sku_blender_bottle';
                buyBtn3.dataset.id = 'sku_blender_bottle';
                // buyBtn1.href = '/order-form/blender-bottle';
