        }
    }

    // Desktop Gallery
    const selectedImg = document.querySelector('#current')
    const images = document.querySelector('.desktop-gallery')
    images.addEventListener('click', activateImage);
    function activateImage(event) {
        if (event.target.src) {
            selectedImg.src = event.target.src
        }
    }

    // Slick carousel
    $(document).ready(function(){
        $photoCarousel = $(document).find('.photo-carousel');
