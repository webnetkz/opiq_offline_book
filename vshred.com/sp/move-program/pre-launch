<script>
		$('.watch-video').on('click', function(e) {
			let timer = 200;
			$('#videoBanner .videoBannerContainer').fadeOut(timer);
			setTimeout(() => {
				$('#videoBanner').addClass('active');
				$('#banner-vsl').addClass('active');
				$('.jwplayer').click()
			}, timer);
        })

        // window.addEventListener('load', setTargetArea, false);
        window.addEventListener('scroll', setTargetArea, false);
        window.addEventListener('resize', setTargetArea, false);

        function setTargetArea() {
            var viewportHeight = window.innerHeight;
            var viewportCenter = viewportHeight/2;
            var targetHeight = 100;
            var targetAreaTop = viewportCenter - (targetHeight/2);
            var targetAreaBtm = targetAreaTop + targetHeight;
            var yOffset = window.pageYOffset;
            var targetEl = document.querySelectorAll('.content-section');

            for(let i=0; i < targetEl.length; i++) {
                let activeEl = targetEl[i];
                if (isInTargetArea(activeEl,targetAreaTop, targetAreaBtm, yOffset)) {
                    activeEl.classList.add('animate');
                    let animList = activeEl.querySelectorAll('.anim-list li');
                    for(let i=0; i < animList.length; i++) {
                        setTimeout(function() {
                            let listItem = animList[i];
                            listItem.classList.add('show');
                        }, 120 * parseInt(i));
                    }
                    let animHdrGlow = activeEl.querySelectorAll('.anim-hdr-glow .hdr-glow');
                    for(let i=0; i < animHdrGlow.length; i++) {
                        setTimeout(function() {
                            let hdrItem = animHdrGlow[i];
                            hdrItem.classList.add('show');
                        }, 500 * parseInt(i));
                    }
                } else {
                    if (activeEl.classList.contains('animate')){
                        activeEl.classList.remove('animate');
                    }
                    // let animList = activeEl.querySelectorAll('.anim-list li');
                    // for(let i=0; i < animList.length; i++) {
                    //     let listItem = animList[i];
                    //     listItem.classList.remove('show');
                    // }
                    let animHdrGlow = activeEl.querySelectorAll('.anim-hdr-glow .hdr-glow');
                    for(let i=0; i < animHdrGlow.length; i++) {
                        let hdrItem = animHdrGlow[i];
                        hdrItem.classList.remove('show');
                    }
                }
            }

        }

        function isInTargetArea(activeEl, targetAreaTop, targetAreaBtm, yOffset) {
            var elTop = activeEl.getBoundingClientRect().top;
            var elBtm = elTop + activeEl.offsetHeight;
            return elBtm >= targetAreaTop && elTop <= targetAreaBtm;
        };
	</script>
<script type="application/javascript">
    // Add internal ref links.
    setTimeout(function() {
        var getParameterByName = function(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }

        $('a').each(function(e) {
            if ($(this).parents('.phpdebugbar, .phpdebugbar-openhandler').length > 0) {
                return;
            }

            var href = $(this).prop('href');

            if (href !== 'undefined' && href.indexOf('#') === -1 && window.location.pathname.indexOf('sp/survey') === -1 && href.indexOf('javascript:') === -1) {
                var ref = '';

                if (window.location.pathname === '/') {
                    ref = 'ref=home';
                } else {
                    ref = 'ref=' + encodeURIComponent(window.location.pathname.substring(1));
                }

                var currentRefValue = getParameterByName('ref', href);
                if (!currentRefValue) {
                    $(this).prop('href', href + (href.indexOf('?') > -1 ? '&' : '?') + ref);
                }
            }
        });
    }, 100);
</script>


